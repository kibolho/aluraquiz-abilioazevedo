{
  "name": "aluraquiz-base",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "eslint:init": "eslint --init",
    "lint": "eslint ./ --fix",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate --schema=src/lib/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api/prisma",
    "migrate:prisma": "prisma migrate dev --name init --preview-feature --schema=./src/lib/prisma/schema.prisma",
    "deploy:migrate:prisma": "prisma migrate deploy --preview-feature --schema=./src/lib/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "src/lib/prisma/schema.prisma"
  },
  "dependencies": {
    "@apollo/client": "3.1.1",
    "@apollo/react-hooks": "4.0.0",
    "@apollo/react-ssr": "4.0.0",
    "@crello/react-lottie": "^0.0.11",
    "@hookform/resolvers": "1.3.0",
    "@prisma/client": "2.15.0",
    "apollo-boost": "0.4.9",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-micro": "2.19.2",
    "axios": "^0.21.1",
    "chrome-aws-lambda": "^5.5.0",
    "contentful": "^8.1.7",
    "deepmerge": "^4.2.2",
    "express": "4.17.1",
    "framer-motion": "^3.2.1",
    "graphql": "15.5.0",
    "graphql-iso-date": "3.6.1",
    "graphql-tag": "2.11.0",
    "lodash": "4.17.20",
    "next": "latest",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.29.0",
    "node-html-parser": "^2.1.0",
    "puppeteer-core": "^5.5.0",
    "react": "^17.0.1",
    "react-apollo": "3.1.5",
    "react-dom": "^16.8.0",
    "react-hook-form": "^6.14.2",
    "react-is": "^16.8.0",
    "styled-components": "^5.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@prisma/cli": "2.15.0",
    "@types/graphql": "14.5.0",
    "@types/node": "13.13.40",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-images": "^1.6.2",
    "postcss": "^8.2.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "ts-migrate": "^0.1.15",
    "typescript": "^4.1.2"
  },
  "license": "MIT"
}
